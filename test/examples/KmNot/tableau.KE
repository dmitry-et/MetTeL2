@l false / priority 0;

@l P @l(~P) /  priority 1;

@l ~(~P) / @l P priority 2;

@l (P|Q) / @l P || @l~P @l Q priority 6;
@l (~(P|Q)) / @l(~P) @l(~Q) priority 2;

@l (~([R]P)) / !R(l,f(l,R,P)) @f(l,R,P) (~P) priority 5;
@l [R]P !R(l,l0)/ @l0 P priority 3;

! ~R(l,l0) !R(l,l0) / priority 1;

! ~(~R)(l,l0) / !R(l,l0) priority 2;

@l [~R]P  @l0 Q/ !R(l,l0) || !~R(l,l0) @l0 P priority 6;

@l P  @l0 Q / {l=l0} priority 4;