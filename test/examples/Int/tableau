@l(T P)  @l(F P) /  priority 0;

@l(T false) / priority 0;

@l(T(P|Q)) / @l(T P) || @l(T Q) priority 4;
@l(F(P|Q)) / @l(F P) @l(F Q) priority 1;

@l(T(P&Q)) / @l(T P) @l(T Q) priority 1;
@l(F(P&Q)) / @l(F P) || @l(F Q) priority 4;

@l(T(P->Q)) R(l,l0) @l0(T P) / @l0(T Q) priority 3;
@l(F(P->Q)) / R(l,f(l,P,Q)) @f(l,P,Q)(T P) @f(l,P,Q)(F Q) priority 1;

@l P / R(l,l) priority 1;
R(l0,l1) R(l1,l2) / R(l0,l2) priority 2;
R(l0,l1) R(l1,l0) / [l0=l1] priority 2;
@l(T P) R(l,l0) / @l0(T P) priority 2;
